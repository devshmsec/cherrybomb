{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "baseUrl": ".",
      "paths": {
        "@assets/*": ["./assets/*"],
        "@components/*": ["./src/components/*"],
        "@interfaces/*": ["./src/components/interfaces/*"],
        "@views/*": ["src/components/views/*"],
        "@redux/*": ["./src/redux/*"],
        "@styles/*": ["./src/styles/*"],
        "@utils/*": ["./src/utils/*"],
        "@src/*": ["./src/*"],
        "@node_modules/*": ["./node_modules/*"]
      }
    },
    "exclude": ["node_modules"]
}
